# DESCRIPTION: Profilo WordPress ottimizzato e compatibile
# Configurazione testata per siti WordPress e WooCommerce

# ==========================================
# AUTENTICAZIONE
# ==========================================
XSRF_TOKEN=""
SESSION_TOKEN=""

# ==========================================
# HEADERS BROWSER STANDARD
# ==========================================
USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/140.0.0.0 Safari/537.36"
ACCEPT="text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7"
ACCEPT_ENCODING="gzip, deflate, br, zstd"
ACCEPT_LANGUAGE="it-IT,it;q=0.9,en-US;q=0.8,en;q=0.7"
CONNECTION="keep-alive"
UPGRADE_INSECURE_REQUESTS="1"

# Security headers per browser moderni
SEC_CH_UA='"Chromium";v="140", "Google Chrome";v="140", "Not?A_Brand";v="99"'
SEC_CH_UA_MOBILE="?0"
SEC_CH_UA_PLATFORM='"Windows"'
SEC_FETCH_DEST="document"
SEC_FETCH_MODE="navigate"
SEC_FETCH_SITE="none"
SEC_FETCH_USER="?1"

# ==========================================
# OPZIONI CONNESSIONE WORDPRESS
# ==========================================
CONNECT_TIMEOUT="20"
MAX_TIME="60"
INSECURE="true"
FOLLOW_REDIRECTS="true"
MAX_REDIRECTS="10"

# ==========================================
# PARAMETRI TESTING WORDPRESS
# ==========================================
DEFAULT_DELAY="1.5"
DEFAULT_PARALLEL_JOBS="3"
MIN_LOG_LEVEL="INFO"
LOG_HEADERS="false"

# Host header sarà impostato automaticamente
HOST=""

# Referer sarà impostato automaticamente
REFERER_URL=""